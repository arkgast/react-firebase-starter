{
  "presets": [
      ["es2015", {"modules": false}],
      "react"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-inline-elements",
        ["transform-runtime", {
          "helpers": true,
          "polifill": false,
          "generator": false,
          "moduleName": "babel-runtime"
        }]
      ]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel",
      ]
    }
  },
}
